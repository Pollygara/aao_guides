    <Script>
      <group>WT_CJ4</group>
      <label>right_rotary_dec</label>
      <script>(L:wt_knob_selector, Number) 1 == if{ 1 (&gt;K:KEY_MINUS, KeyEvent) } (L:wt_knob_selector, Number) 2 == if{ 1 (&gt;K:KEY_MINUS, KeyEvent) } (L:wt_knob_selector, Number) 3 == if{ 1 (&gt;K:HEADING_BUG_DEC) } (L:wt_knob_selector, Number) 5 == if{ 1 (&gt;K:KEY_MINUS, KeyEvent) }</script>
    </Script>
    <Script>
      <group>WT_CJ4</group>
      <label>right_rotary_inc</label>
      <script>(L:wt_knob_selector, Number) 1 == if{ 1 (&gt;K:KEY_PLUS, KeyEvent) } (L:wt_knob_selector, Number) 2 == if{ 1 (&gt;K:KEY_PLUS, KeyEvent) } (L:wt_knob_selector, Number) 3 == if{ 1 (&gt;K:HEADING_BUG_INC) } (L:wt_knob_selector, Number) 5 == if{ 1 (&gt;K:KEY_PLUS, KeyEvent) }</script>
